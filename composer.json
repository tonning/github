{
    "name": "tonning/github",
    "description": "Laravel GitHub SDK",
    "keywords": [
        "tonning",
        "github"
    ],
    "homepage": "https://github.com/tonning/github",
    "license": "MIT",
    "authors": [
        {
            "name": "Laura Tonning",
            "email": "tonning@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Tonning\\Github\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonning\\Github\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
